<p>Nos restaurants partenaires</p>

<%@restaurants.each do |restaurant| %>
<ul>
  <li>name : <%= restaurant.name%></li>
  <li>address : <%= restaurant.adress%></li>
  <li>phone : <%= restaurant.phone%></li>
  <li>category : <%= restaurant.category%></li>
  <li><%=link_to "show restaurant", resto_path(restaurant)%></li>
</ul>
<%end%>

<%= link_to "Add a restaurant", :action => "new" %>